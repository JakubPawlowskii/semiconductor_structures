doi = {10.1016/j.nimb.2005.11.131},
file = {:home/jakubp/.var/app/com.elsevier.MendeleyDesktop/data/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Dubey et al. - 2006 - Study of optical properties of swift heavy ion irradiated gallium antimonide.pdf:pdf},
issn = {0168583X},
keywords = {Annealing,Defects,FTIR,Gallium antimonide,High-energy ion irradiation,Iron},
month = {feb},
number = {1},
pages = {141--144},
publisher = {North-Holland},
title = {{Study of optical properties of swift heavy ion irradiated gallium antimonide}},
volume = {244},
year = {2006}
}
