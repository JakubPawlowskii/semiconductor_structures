doi = {10.1109/JQE.1981.1071074},
issn = {15581713},
journal = {IEEE Journal of Quantum Electronics},
number = {2},
pages = {275--283},
title = {{The GaAIAsSb Quaternary and GaAISb Ternary Alloys and Their Application to Infrared Detectors}},
volume = {17},
year = {1981}
}
