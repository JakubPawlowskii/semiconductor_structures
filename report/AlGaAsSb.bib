@article{Wootten2014,
abstract = {High power, high radiance, broadband light sources emitting in the 2.0-2.5 $\mu$m wavelength range are important for optical sensing of biomolecules such as glucose in aqueous solutions. Here we demonstrate and analyze superluminescent diodes with output centered at 2.4 $\mu$m (range ∼2.2-2.5 $\mu$m) from GaInAsSb/AlGaAsSb quantum wells in a separate confinement structure. Pulsed wave output of 1 mW (38 kW cm2 Sr-1) is achieved at room temperature for 40 $\mu$m × 2 mm devices. Superluminescence is evidenced in superlinear increase in emission, spectral narrowing, and angular narrowing of light output with increasing current injection. Optical output is analyzed and modeled with rate equations. Potential routes for future improvements are explored, such as additional Auger suppression and photonic mode engineering.},
author = {Wootten, M. B. and Tan, J. and Chien, Y. J. and Olesberg, J. T. and Prineas, J. P.},
doi = {10.1088/0268-1242/29/11/115014},
file = {:home/jakubp/.var/app/com.elsevier.MendeleyDesktop/data/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Wootten et al. - 2014 - Broadband 2.4 $\mu$m superluminescent GaInAsSbAlGaAsSb quantum well diodes for optical sensing of biomolecules.pdf:pdf},
issn = {13616641},
journal = {Semiconductor Science and Technology},
keywords = {gallium antimonide,glucose sensing,light emitting diode,superluminescence},
month = {nov},
number = {11},
publisher = {Institute of Physics Publishing},
title = {{Broadband 2.4 $\mu$m superluminescent GaInAsSb/AlGaAsSb quantum well diodes for optical sensing of biomolecules}},
url = {/pmc/articles/PMC4283575/ /pmc/articles/PMC4283575/?report=abstract https://www.ncbi.nlm.nih.gov/pmc/articles/PMC4283575/},
volume = {29},
year = {2014}
}
